{
  "name": "@toiletmap/hono-api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "prestart": "pnpm build",
    "check": "tsc --noEmit",
    "docs:generate": "tsx src/docs/generate.ts",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:db:pull": "prisma db pull --schema prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma",
    "supabase:start": "SUPABASE_SCANNER_BUFFER_SIZE=100mb supabase start --exclude gotrue,realtime,storage-api,imgproxy,kong,mailpit,postgrest,postgres-meta,studio,edge-runtime,logflare,vector,supavisor",
    "supabase:stop": "supabase stop",
    "supabase:reset": "SUPABASE_SCANNER_BUFFER_SIZE=100mb supabase db reset",
    "test:e2e": "vitest run"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^1.1.4",
    "@hono/zod-validator": "^0.7.5",
    "@prisma/client": "^6.19.0",
    "dotenv": "^17.2.3",
    "hono": "^4.10.6",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "openapi3-ts": "^4.5.0",
    "prisma": "^6.19.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "22.10.2",
    "supabase": "^2.58.5",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vitest": "^4.0.9"
  }
}

# Copy to `.env.local` or `.env` and update with your connection string.
POSTGRES_URI=postgresql://postgres:postgres@localhost:54322/postgres
# Required Auth0 configuration.
AUTH0_ISSUER_BASE_URL=http://127.0.0.1:44555/
AUTH0_AUDIENCE=toiletmap-api

# Optional server port (defaults to 4001)
# PORT=4001

# Optional Auth0-powered data explorer configuration shown in our admin explorer.
AUTH0_DATA_EXPLORER_CLIENT_ID=
AUTH0_DATA_EXPLORER_SCOPE=openid profile email offline_access
AUTH0_DATA_EXPLORER_REDIRECT_URI=http://localhost:4001/admin/callback
AUTH0_PROFILE_KEY=app_metadata

# Playwright E2E Testing - Auth0 credentials for admin user
PLAYWRIGHT_AUTH0_USERNAME=
PLAYWRIGHT_AUTH0_PASSWORD=
